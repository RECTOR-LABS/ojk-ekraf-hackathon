{
  "name": "frontend",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "eslint",
    "typecheck": "tsc --noEmit --strict --noUnusedLocals --noUnusedParameters --noUncheckedIndexedAccess --noImplicitReturns --noFallthroughCasesInSwitch",
    "typecheck:watch": "tsc --noEmit --strict --noUnusedLocals --noUnusedParameters --noUncheckedIndexedAccess --noImplicitReturns --noFallthroughCasesInSwitch --watch",
    "typecheck:basic": "tsc --noEmit",
    "check": "npm run lint && npm run typecheck",
    "check:all": "npm run lint && npm run typecheck && npm run build"
  },
  "dependencies": {
    "@radix-ui/react-slot": "^1.2.3",
    "@rainbow-me/rainbowkit": "^2.2.8",
    "@tanstack/react-query": "^5.90.2",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "lucide-react": "^0.545.0",
    "next": "15.5.4",
    "pinata": "^2.5.1",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "sonner": "^2.0.7",
    "tailwind-merge": "^3.3.1",
    "viem": "^2.38.1",
    "wagmi": "^2.18.0",
    "zustand": "^5.0.8"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@tailwindcss/postcss": "^4",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "eslint": "^9",
    "eslint-config-next": "15.5.4",
    "tailwindcss": "^4",
    "typescript": "^5"
  }
}
