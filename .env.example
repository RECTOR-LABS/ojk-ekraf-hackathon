# Sepolia Testnet Configuration
# Copy this file to .env and fill in your values

# Your wallet private key (DO NOT share this!)
# Get from MetaMask: Account Details > Export Private Key
PRIVATE_KEY=your_private_key_here

# Sepolia RPC URL (optional - default is https://rpc.sepolia.dev)
# Get free RPC from:
# - Alchemy: https://dashboard.alchemy.com/
# - Infura: https://infura.io/
SEPOLIA_RPC_URL=https://rpc.sepolia.dev

# Etherscan API Key (required for contract verification)
# Get from: https://etherscan.io/myapikey
ETHERSCAN_API_KEY=your_etherscan_api_key_here

# Optional: Gas reporting
REPORT_GAS=false
COINMARKETCAP_API_KEY=
